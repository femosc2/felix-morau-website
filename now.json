{
    "version": 2,
    "alias": "www.felixmorau.se",
    "name": "vue",
    "builds": [
        { "src": "package.json", "use": "@now/static-build" }
    ],
    "routes": [
      {"src": "/dist/^/js/(.*)", "dest": "/js/$1"},
      {"src": "/dist/^/css/(.*)", "dest": "/css/$1"},
      {"src": "/dist/^/img/(.*)", "dest": "/img/$1"},
      {"src": "/dist/.*", "dest": "/index.html"}
    ]
}